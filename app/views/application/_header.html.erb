<header id = "header">
  <h1>Movies</h1>
  <p>
    <% if current_user %>
      <%= link_to current_user.name, user_path(current_user) %>
      <%= link_to "Account Settings", edit_user_path(current_user) %>
      <%= link_to "Sign Out", session_path, method: :delete %>
    <% else %>
      <%= link_to "Sign In", signin_path %>
      <%= link_to "Sign Up", signup_path %>
    <% end %>
  </p>

</header>
